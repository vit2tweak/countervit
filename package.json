{
  "name": "countervit",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A modern counter application built with React, TypeScript, and Vite",
  "keywords": ["react", "typescript", "vite", "counter", "web-app"],
  "author": "Developer",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
